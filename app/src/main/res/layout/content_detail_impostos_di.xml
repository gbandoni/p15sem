        android:layout_height="wrap_content"
        android:text="@string/j34_siscomex_impostos_di_Valormultapagamentotributo_text"
        android:id="@+id/lblValormultapagamentotributo"
        android:layout_below="@+id/edtValorjurospagamentotributo"
